{
  "name": "hello-books",
  "version": "1.0.0",
  "engines": {
    "node": "6.11.0",
    "npm": "3.10.10"
  },
  "description": "A simple application that helps manage a library and its processes like stocking, tracking and renting books.",
  "scripts": {
    "coverage": "NODE_ENV=test nyc --reporter=lcov --reporter=text --reporter=lcovonly mocha server/dist/api/v1/tests/index.js",
    "webpack": "webpack",
    "babel": "babel server/src/ --watch --out-dir server/dist/",
    "watch": "nodemon --watch server/dist ./server/dist/server.js",
    "start": "NODE_ENV=production node ./server/dist/server.js",
    "build": "babel server/src/ --out-dir server/dist/",
    "heroku-postbuild": "cd client && npm install && npm run build && cd ../server && npm run build",
    "test:dev": "SET NODE_ENV=test&& mocha server/dist/api/v1/tests/index.js --timeout 100000",
    "test": "NODE_ENV=test mocha server/dist/api/v1/tests/index.js --timeout 100000",
    "test:db": "sequelize db:migrate --env test"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adesege/HelloBooks.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adesege/HelloBooks/issues"
  },
  "homepage": "https://github.com/adesege/HelloBooks#readme",
  "devDependencies": {
    "chai-http": "^3.0.0",
    "codeclimate-test-reporter": "^0.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-config-react-app": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.11.0",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.2",
    "chai": "^4.1.1",
    "cloudinary": "^1.9.0",
    "connect": "^3.6.3",
    "dotenv": "^4.0.0",
    "ejs": "^2.5.7",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "express": "^4.15.3",
    "faker": "^4.1.0",
    "js-yaml": "^3.10.0",
    "jsonwebtoken": "^7.4.1",
    "mocha": "^3.5.0",
    "morgan": "^1.8.2",
    "nyc": "^11.1.0",
    "pg": "^7.1.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.4.2",
    "sequelize-cli": "^2.8.0",
    "superagent": "^3.6.0",
    "supertest": "^3.0.0",
    "swagger-tools": "^0.10.1",
    "webpack": "^2.7.0"
  }
}
